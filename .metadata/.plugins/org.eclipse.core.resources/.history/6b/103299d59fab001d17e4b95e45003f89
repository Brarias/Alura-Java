
public class EjercicioFactorial {
	public static void main(String[] args) {
		/* Todos los factoriales de 1 al 10 
		 *ejemplo = El factorial de 4! = 1 x 2 x 3 x 4 = 24
		 * */
		
		for(int i = 1; i <= 10; i++) {
			for(int j = 1; j <= i; j++) {
				System.out.println("factorial de "+i+ " = "+i * j);
			}
		}
		
	}
}
